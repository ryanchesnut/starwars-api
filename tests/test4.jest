import { getGalaxyPopulation } from './example'

describe('getGalaxyPopulation', () => {
  it('should return a string representing the total population of all known planets', async () => {
    const result = await getGalaxyPopulation()
    expect(typeof result).toBe('string')
    expect(result).not.toBe('')
  })

  it('should return null if an error occurs', async () => {
    jest.spyOn(global.console, 'error').mockImplementation(() => {})
    const result = await getGalaxyPopulation()
    expect(result).toBe(null)
    expect(global.console.error).toHaveBeenCalledTimes(1)
    jest.spyOn(global.console, 'error').mockRestore()
  })
})
