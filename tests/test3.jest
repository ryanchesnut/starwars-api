import { getNumberOfPlanets } from './example'

describe('getNumberOfPlanets', () => {
  it('should return the number of planets', async () => {
    const result = await getNumberOfPlanets()
    expect(result).toBeGreaterThan(0)
  })

  it('should return null if an error occurs', async () => {
    const result = await getNumberOfPlanets('invalid_argument')
    expect(result).toBe(null)
  })
})
